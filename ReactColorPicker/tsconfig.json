{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": false,
    "types": [ "webpack-env" ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
